---
# Cribl Edge Splunk HEC output configuration
# Generated by Ansible

outputs:
  splunk_hec:
    type: splunk-hec
    url: {{ cribl_edge_hec_url }}
    token: {{ cribl_edge_hec_token }}
    tlsVerify: {{ cribl_edge_hec_tls_verify | lower }}
    batchSize: 1000
    maxConnections: 4
    timeout: 30
    compression: gzip
