---
# Cribl Edge routes configuration
# Generated by Ansible - cribl_docker_stack role
# Routes connect inputs -> pipelines -> outputs

routes:
  # Default route: all events go through syslog pipeline to Splunk
  - id: default
    name: default
    filter: "true"
    pipeline: syslog
    output: splunk_hec
    final: true
    disabled: false

groups: {}
